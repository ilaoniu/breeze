{
    "name": "ilaoniu/breeze",
    "description": "Minimal Laravel scaffolding with I-UI and Tailwind.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ilaoniu/breeze/issues",
        "source": "https://github.com/ilaoniu/breeze"
    },
    "authors": [
        {
            "name": "ilaoniu",
            "email": "ilaoniu@qq.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/validation": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ILaoniu\\Breeze\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ILaoniu\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.10"
    }
}
